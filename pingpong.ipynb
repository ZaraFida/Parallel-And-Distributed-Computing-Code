from mpi4py import MPI
comm = MPI.COMM_WORLD
rank = comm.Get_rank()
number = 0
while number < 10:
if rank == 0:
number += 1
comm.send(number, dest=1)
number = comm.recv(source=1)
else:
number = comm.recv(source=0)
number += 1
comm.send(number, dest=0)
if rank == 0:
print(f"Final number after ping-pong: {number}")